<Window x:Class="Sanity.Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sanity.Editor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu x:Name="MainMenuBar" HorizontalAlignment="Left" VerticalAlignment="Top">
            <MenuItem FontSize="14" Header="Project"></MenuItem>
            <MenuItem FontSize="14" Header="World"></MenuItem>
            <MenuItem FontSize="14" Header="Environment"></MenuItem>
            <MenuItem FontSize="14" Header="AI"></MenuItem>
        </Menu>
    </Grid>
</Window>
